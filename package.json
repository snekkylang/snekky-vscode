{
    "name": "snekky-lang",
    "displayName": "Snekky Language",
    "description": "The Snekky Programming Language",
    "publisher": "senkju",
    "version": "0.5.0",
    "icon": "icon.png",
    "license": "MIT",
    "keywords": [
        "snekky",
        "syntax"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/snekkylang/snekky-vscode"
    },
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "snekky",
            "aliases": ["Snekky", "snekky"],
            "extensions": [".snek"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "snekky",
            "scopeName": "source.snek",
            "path": "./syntaxes/snekky.tmLanguage.json"
        }]
    }
}